CREATE TABLE IF NOT EXISTS `generos_anime` (
  `id_Anime` int(11) NOT NULL,
  `id_Genero` int(11) NOT NULL,
  PRIMARY KEY (`id_Anime`,`id_Genero`),
  KEY `id_Genero` (`id_Genero`),
  CONSTRAINT `generos_anime_ibfk_1` FOREIGN KEY (`id_Anime`) REFERENCES `animes` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `generos_anime_ibfk_2` FOREIGN KEY (`id_Genero`) REFERENCES `genero` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4;

-- Volcando datos para la tabla proyectoanime.generos_anime: ~436 rows (aproximadamente)
/*!40000 ALTER TABLE `generos_anime` DISABLE KEYS */;
